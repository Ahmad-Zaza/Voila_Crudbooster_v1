{
    "name": "crud_booster_voila/crudbooster",
    "description": "CRUDBooster Is CRUD Generator Based On Laravel Easy To Make An Advanced Backend",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "admin-panel",
        "backend",
        "crud-generator",
        "crudbooster",
        "crud",
        "generator"
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "barryvdh/laravel-dompdf": "0.8.*",
        "doctrine/dbal": "~2.3",
        "maatwebsite/excel": "~2.1.0",
        "unisharp/laravel-filemanager": "1.7.7",
        "intervention/imagecache": "2.3.3",
        "imanghafoori/laravel-heyman": "2.*",
        "imanghafoori/laravel-widgetize": "1.8.*"
    },
    "autoload": {
        "psr-4": {
            "crud_booster_voila\\crudbooster\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "crud_booster_voila\\crudbooster\\CRUDBoosterServiceProvider"
            ]
        }
    }
}
